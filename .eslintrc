{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
  "flowtype"
  ]
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "import/no-extraneous-dependencies": [2 , { "devDependencies": true }]
  },
  "settings": {
      "onlyFilesWithFlowAnnotation": true
    }
}
